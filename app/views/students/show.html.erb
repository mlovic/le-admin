<div class="page-header">
  <%= link_to students_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_student_path(@student), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show student</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @student.name %></dd>

  <dt>Phone:</dt>
  <dd><%= @student.phone %></dd>

  <dt>Level:</dt>
  <dd><%= @student.level %></dd>

  <dt>Notes:</dt>
  <dd><%= @student.notes %></dd>

  <dt>Group:</dt>
  <dd><%= @student.group_id %></dd>

</dl>
